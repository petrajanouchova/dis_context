@incollection{Renfrew1986,
abstract = {Mode of interaction between the two autonomous and independent political units on the same level, located next to each other. The interaction include: imitation, eulation, competition, warfare, exchange of material goods and interaction. The dominance of one polity over another is not focal point of this approach, alyhough it is one of the possible modes of contact. Autonomous unit: village, tribal unit, chiefdom, city, state, empire. The term polity is not dependent on the level of social complexity of the autonomous political-unit. The polity - the highest form of socio-political organisation in the given region (often village or tribe). More complex polities have form of social hierarchy - they may consist of old polities that merged under one authority. Polity does not need ot be distinguished by developed system of governmnet or administration. The procedures of decision making have to habitually work, and such decisions have to be respected by the members of the polity. The adjacent small polities often share a number of features: language, symbolic systems, including belief, therefore they are not easily recoverable by means of archaeology. Change is caused by internal or external factors (endogenous and exogenous agent): Exogenous: diffusion of culture, core/periphery. Endogenous: internal growth etc. Peer polity: between the two, the change is happening within the polity, but it has effect also to neighbourhooding polities. The changes are more or less contemporary, and it is hard to track the original polity (the= single locus of innovation). Such changes include architectural style, conceptual systems of commmunication information (writing, system of measures), assemblages of specific and special artefacts which may be associated with high status in the society in question, customs (ritual practices). The mode of interaction: a) competition and competitive emulation b) symbolic entrainment, transmission of innovation c) increased flow in the exchange of goods. Nature of interactions: * warfare * competitive emulation - display of wealth and power to achieve higher inter-polity status, immitation of neighbour in a greater manner, conspicuous consumption and display * symbolic entrainment - developed symbolic system adopted by other the less developed one, writing, systems of measures, social organisation, political system, adopted to bring an advantage and prestige. * transmission of innovation - innovations not of a symbolic nature, technology * increased flow and exchange of goods - import, creation of new institutions to cope with the reception, allocation and distribution; craft specialization},
address = {Cambridge},
author = {Renfrew, Colin},
booktitle = {Peer Polity Interaction and Socio‐Political Change},
editor = {Renfrew, Colin and Cherry, John F.},
pages = {1--18},
publisher = {Cambridge University Press},
title = {{Introduction: Peer Polity Interaction and Socio-Political Change}},
year = {1986}
}
